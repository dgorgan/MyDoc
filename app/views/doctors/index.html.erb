<% @doctors.each do |doctor| %>
  <div class="row">
    <div class="col-md-3">
        <img class="doctor-img" src= <%= doctor.photo %> />
    </div>
    <div class="col-md-6">
       <h1><%= doctor.name %></h1>
       <h3><span class="glyphicon glyphicon-heart-empty glyph-style" aria-hidden="true"></span> <%= doctor.specialty %></h3>
         <div class="well text-center doctor-description-style">
            <p>
              <h3 class="text-center">About me </h3>
               <%= doctor.description %>
            </p>
      </div>
    </div>
  </div>
<% end %>
